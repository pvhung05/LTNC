#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {

    int n, sum1 =0, sum2 =0;
    cin >> n;
    int a[n+1],b[n+2];
    for(int i=0;i<n;i++) {
        cin >> a[i];
        sum1+=a[i];
    }
    for(int i=0;i<=n;i++) {
        cin >> b[i];
        sum2+=b[i];
    }
    cout << sum2-sum1;
    return 0;
}
