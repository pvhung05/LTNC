#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
bool check_new_space(int n, int m, vector<string>& arr )
{
    pair<int, int> playerPos;
    for(int i=0;i<n;i++) 
    {
        for(int j=0;j<m;j++)
        {
            if(arr[i][j]=='Y') 
            {
                playerPos = make_pair(i, j);
                break;
            }
        }
    }
    while(playerPos.first < n-1)
    {
        if(arr[playerPos.first+1][playerPos.second-1]=='R' && arr[playerPos.first+1][playerPos.second-1]=='R' && arr[playerPos.first+1][playerPos.second-1]=='R')
        {
            return false;
        }else return true;
    }
    return true;
}

int main() {
    int n,m;
    cin >> n >> m;
    vector<string> arr(n);
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    if(check_new_space(n, m, arr )==true) cout <<"YES"<<endl;
    else cout <<"NO"<<endl;
    return 0;
}
