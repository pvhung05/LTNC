#include <iostream>
#include <iomanip>
#include <limits>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
    int a; cin >> a;
    double b;cin >> b;
    string k;
    cin.ignore();
    getline(cin , k);
    cout << i+a << endl;
    cout <<fixed << setprecision(1) <<  double(d+b) << endl;
    cout << s << k << endl;
    return 0;
}
