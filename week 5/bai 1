#include <iostream>
#include <cmath>
#include <bits/stdc++.h>

using namespace std;

void calculation(int *a, int *b)
{
    int s=*a - *b;
    cout << *a + *b << endl;
    if(s>0) cout << s;
    else cout << -s;
}
int main () {
    int a, b;
    cin >> a >> b;
    calculation(&a, &b);
    return 0;
}
